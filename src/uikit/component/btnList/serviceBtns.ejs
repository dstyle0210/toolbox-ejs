<% 
const {btns} = locals;
%>
<div class="c-serviceBtns swiper" id="serviceBtns">
    <ul class="swiper-wrapper">
        <% btns.forEach((btn)=>{%>
        <li class="swiper-slide">
            <include src="../../atom/btn/serviceBtn.ejs" props="btn"></include>
        </li>
        <%});%>
    </ul>
</div>
<script>
  $(()=>{
    new Swiper("#serviceBtns",{
      slidesPerView:5,
      spaceBetween:10,
      grid:{
        rows:2
      },
    })
  })
</script>